<form id="itemFindForm" action="javascript:void(0)">
    <div class="grid-x grid-padding-x">
        <div class="cell large-6 small-6">
            <span class="label">Item Maker</span>
            <input type="text" id="findItemMaker" maxlength="10">
        </div>
        <div class="cell large-6 small-6">
            <span class="label">Item Model</span>
            <input type="text" id="findItemModel" maxlength="10">
        </div>
    </div>
    <div class="grid-x grid-padding-x">
        <div class="cell large-12 small-12">
            <span class="label">Asset Tag</span>
            <input type="number" id="findItemAssetTag" maxlength="10">
        </div>
    </div>
</form>
<div class="grid-x grid-padding-x grid-padding-y">
    <div class="cell large-12 small-12">
        <input type="submit" class="success_bdh button large expanded" id="itemFindSubmit" value="Find Item">
    </div>
</div>